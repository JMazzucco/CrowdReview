<div class="search-bar">
	<h4>My Feed</h4>
  <div class="keyword-form">
		<%= form_tag user_path(current_user.id), :method => :patch, id: 'search-form'  do |f| %>
		<%= text_field_tag :keyword %>
		<% end %>
	  </div>
	</div>
</div>

<div id="keywords">
	<%= render partial: "keyword" %>
</div>

<hr/>
<h5><%= @articles.count %> articles</h5>
<hr/>

<div id="articles">
	<%= render @articles %>
</div>



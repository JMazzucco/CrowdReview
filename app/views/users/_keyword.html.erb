	<% @keywords.each do |keyword| %>
		<% @keyword_index = @keywords.index(keyword) %>
			<div class="keyword">
				<div class="key-text">
					<%= keyword %>
					<%= link_to user_path(index: @keyword_index, id: current_user.id), :method => :delete do %>
						<span class="delete">X</span>
					<% end %>
				</div>
				<div class="key-delete">

			</div>
			</div>
	<% end %>
